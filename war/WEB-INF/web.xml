		<servlet-class>pt.lsts.ripples.servlets.LogbookServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LogBook</servlet-name>
		<url-pattern>/logbook/*</url-pattern>
	</servlet-mapping>
	
