		<servlet-class>pt.lsts.ripples.servlets.datastore.DataStoreServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DataStoreServlet</servlet-name>
		<url-pattern>/datastore/*</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
