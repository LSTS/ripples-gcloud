	</servlet>
	<servlet-mapping>
		<servlet-name>RandomPositionsServlet</servlet-name>
		<url-pattern>/addpos/*</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>WavyPositionsServlet</servlet-name>
